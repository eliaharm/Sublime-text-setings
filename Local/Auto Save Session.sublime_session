LocCurr      NUmber(3);\n  V_CNAT_CODE    NUmber(3);\n  V_LocBrch      NUmber(3);\n  V_Count_Oper      NUmber(7);\n  V_Count_errors      NUmber(7);\n  \nbegin\n  \n  V_Count_Oper :=0;\n  V_Count_errors :=0;\n  \n  :s_status := 'Processing';\n  --:parameter.p_date := :s_date;\n  synchronize ;\n  dv_error_log('Started: \" transfer  \"');\n  --the following are important\n  :h_opr_nat := 1;\n  :h_need_line := 1;-- needed to operate\n  :h_need_advice := 2;\n  :h_flag_2 := 1;-- if canceled we get the message \"account can not be debitor\"-- if 1 => No Overdraw -- if 0 => Overdraw request\n  :h_flag_3 := 1;   \n  :line_seq :=1;  \n      \n  BEGIN\n    SELECT  SYST_NUMBER\n      INTO  V_LocCurr\n      FROM  GBSS_SYSTEMS\n     WHERE SYST_CODE = 'LOCCURR';\n  EXCEPTION\n    WHEN OTHERS THEN V_LocCurr := 0;\n  END;\n     \n  BEGIN\n    select ud.brch_code\n      INTO  V_LocBrch\n      from gbss_users_defs  ud\n     where ud.udef_code = user;\n  EXCEPTION\n    WHEN OTHERS THEN V_LocBrch := 0;\n  END;\n\n  for data_tran in cur_tran \n   loop    \n    BEGIN   -- end with e_skip to rollback current operation\n      V_Count_Oper :=V_Count_Oper+1;\n      p_log_Star_Seq := DV_ONLINE_TRANS_LOG_T('OT_BG_TRN',\n                              'Beging generating transaction', --IN_LOG_DESC \n                              data_tran.V_FrmAccId ,--IN_ORDERED_BY_CUST_ID , \n                              data_tran.V_TodayDate,\n                              data_tran.V_FrmBrch,   --IN_BRCH_CODE\n                              data_tran.V_FrmCacc,   --IN_CACC_NUM_FROM \n                              data_tran.V_FrmAccCurr,   --IN_CURR_CODE_FROM\n                              data_tran.V_FrmAccAmnt,   --IN_AMNT_VAR_FROM \n                              data_tran.V_ToBrch,   --IN_BRCH_CODE_TO\n                              data_tran.V_ToCacc,   --IN_CACC_NUM_TO \n                              data_tran.V_ToAccCurr,   --IN_CURR_CODE_TO\n                              data_tran.V_ToAccAmnt,   --IN_AMN