an.V_ToAccAmnt,   --IN_AMNT_VAR_TO\n                              data_tran.V_From_Reser_ref,   --IN_RESER_REF_FROM\n                              data_tran.V_To_Reser_ref,   --IN_RESER_REF_TO\n                              data_tran.V_Star_Seq,   --IN_JOIN_SEQ \n                              0 --IN_RESP_CODE \n                              );\n        no_message('commit_form');\n     else\n        error_display(v_error,1,false);--true\n        begin\n          select errm_desc\n            into v_errm_desc\n            from gbss_error_messages\n           where errm_code = v_error\n             and rownum =1;   \n        exception when others then null;\n        end;     \n        V_Count_errors := V_Count_errors +1;\n        p_log_Star_Seq := DV_ONLINE_TRANS_LOG_T('OT_ER_GEN',\n                            'Error gen transactions for joint seq '||data_tran.V_Star_Seq||\n                            ' ErrNo '||v_error||' '||v_errm_desc, --IN_LOG_DESC \n                            data_tran.V_FrmAccId ,--IN_ORDERED_BY_CUST_ID , \n                            data_tran.V_TodayDate,\n                            data_tran.V_FrmBrch,   --IN_BRCH_CODE\n                            data_tran.V_FrmCacc,   --IN_CACC_NUM_FROM \n                            data_tran.V_FrmAccCurr,   --IN_CURR_CODE_FROM\n                            data_tran.V_FrmAccAmnt,   --IN_AMNT_VAR_FROM \n                            data_tran.V_ToBrch,   --IN_BRCH_CODE_TO\n                            data_tran.V_ToCacc,   --IN_CACC_NUM_TO \n                            data_tran.V_ToAccCurr,   --IN_CURR_CODE_TO\n                            data_tran.V_ToAccAmnt,   --IN_AMNT_VAR_TO\n                            data_tran.V_From_Reser_ref,   --IN_RESER_REF_FROM\n                            data_tran.V_To_Reser_ref,   --IN_RESER_REF_TO\n                            data_tran.V_Star_Seq,   --IN_JOIN_SEQ \n                            -2--IN_RESP_CODE \n                            );\n        RAISE E_Skip;     \n     